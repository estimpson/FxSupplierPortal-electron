{
    "executable": "FxSupplierPortal-electron",
    "splashscreen": {
        "imageFile": "/public/favicon.png"
    },
    "name": "FxSupplierPortal-electron",
    "author": "",
    "singleInstance": false,
    "environment": "Production",
    "build": {
        "appId": "com.FxSupplierPortal-electron.app",
        "productName": "FxSupplierPortal-electron",
        "copyright": "Copyright Â© 2021",
        "buildVersion": "0.3.1",
        "compression": "maximum",
        "win": {
            "icon": "../../../public/favicon.png",
            "publish": [
                {
                    "provider": "github",
                    "owner": "estimpson",
                    "repo": "FxSupplierPortal-electron"
                }
            ]
        },
        "directories": {
            "output": "../../../bin/Desktop"
        },
        "extraResources": [
            {
                "from": "./bin",
                "to": "bin",
                "filter": ["**/*"]
            }
        ],
        "files": [
            {
                "from": "./ElectronHostHook/node_modules",
                "to": "ElectronHostHook/node_modules",
                "filter": ["**/*"]
            },
            "**/*"
        ]
    }
}
